def countlengthofcycle(arr,index):

    count=1
    for i in arr:
        
        if(i<0):
            return None
        else:
            if(i!=index):
                count+=1
            else:
                return count
    return 0
    
                


arr=[1,0,2,3]
index=3
#3
print(countlengthofcycle(arr,index))
